{
  "name": "denverdevshop",
  "version": "1.0.0",
  "repository": {
    "type": "git",
    "url": "git://github.com/noahschutte/denverdevshop.git"
  },
  "description": "Server side rendering company website",
  "main": "src/index.js",
  "engines": {
    "node": "8.11.3",
    "npm": "5.6.0"
  },
  "scripts": {
    "dev": "npm-run-all --parallel dev:*",
    "dev:server": "nodemon --watch build --exec node build/bundle.js",
    "dev:build-server": "webpack -p --config webpack.server.js --watch",
    "dev:build-client": "webpack -p --config webpack.client.js --watch",
    "heroku-postbuild": "webpack -p --config webpack.server.js && webpack -p --config webpack.production.client.js",
    "lint": "eslint .",
    "lint-fix": "eslint . --fix",
    "start": "nodemon --watch build --exec node build/bundle.js",
    "deploy": "git push heroku master:master",
    "open": "heroku open -a denverdevshop",
    "clean": "rm -rf build/ && rm -rf public/",
    "prettier": "prettier-standard 'src/client/**/*.js'",
    "build": "webpack -p --config webpack.server.js && webpack -p --config webpack.production.client.js"
  },
  "author": "Noah Schutte",
  "license": "ISC",
  "dependencies": {
    "babel-cli": "6.26.0",
    "babel-core": "6.26.0",
    "babel-loader": "7.1.2",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-preset-env": "1.6.0",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-es2017": "6.24.1",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-0": "6.24.1",
    "body-parser": "^1.18.3",
    "compression": "^1.7.3",
    "concurrently": "3.5.0",
    "cors": "^2.8.4",
    "css-loader": "^1.0.0",
    "dotenv": "^6.1.0",
    "express": "^4.17.3",
    "express-http-proxy": "1.0.6",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.11",
    "image-webpack-loader": "^4.3.1",
    "lodash": "^4.17.11",
    "log": "^1.4.0",
    "node-sass": "^4.12.0",
    "nodemailer": "^4.6.8",
    "nodemailer-smtp-transport": "^2.7.4",
    "nodemon": "^1.19.1",
    "npm-run-all": "^4.1.3",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "prop-types": "^15.6.2",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-helmet": "5.2.0",
    "react-jss": "^8.6.1",
    "react-router-config": "1.0.0-beta.4",
    "react-router-dom": "4.2.2",
    "react-router-hash-link": "^1.2.0",
    "sass-loader": "^7.1.0",
    "stats-webpack-plugin": "^0.7.0",
    "style-loader": "^0.23.0",
    "webpack": "3.5.6",
    "webpack-merge": "4.1.0",
    "webpack-node-externals": "1.6.0",
    "xoauth2": "^1.2.0"
  },
  "devDependencies": {
    "babel-eslint": "^9.0.0",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^2.10.0",
    "eslint-config-react": "^1.1.7",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.11.1",
    "prettier-standard": "^8.0.1",
    "webpack-dev-server": "2.8.2"
  }
}

{
  "name": "DenverDevShop",
  "version": "1.0.0",
  "description": "Server side rendering company website",
  "main": "src/index.js",
  "scripts": {
    "dev": "npm-run-all --parallel dev:*",
    "dev:server": "nodemon --watch build --exec node build/bundle.js",
    "dev:build-server": "webpack --config webpack.server.js --watch",
    "dev:build-client": "webpack --config webpack.client.js --watch",
    "heroku-postbuild": "webpack --config webpack.server.js && webpack --config webpack.client.js",
    "lint": "eslint .",
    "lint-fix": "eslint . --fix",
    "start": "node build/bundle.js",
    "deploy": "git push heroku master:master",
    "open": "heroku open -a denverdevshopsite"
  },
  "author": "Noah Schutte",
  "license": "ISC",
  "dependencies": {
    "babel-cli": "6.26.0",
    "babel-core": "6.26.0",
    "babel-loader": "7.1.2",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-preset-env": "1.6.0",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-es2017": "6.24.1",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-0": "6.24.1",
    "compression": "1.7.0",
    "concurrently": "3.5.0",
    "css-loader": "^1.0.0",
    "express": "4.15.4",
    "express-http-proxy": "1.0.6",
    "lodash": "^4.17.10",
    "log": "^1.4.0",
    "node-sass": "^4.9.3",
    "nodemon": "1.12.0",
    "npm-run-all": "^4.1.3",
    "prop-types": "^15.6.2",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "react-helmet": "5.2.0",
    "react-jss": "^8.6.1",
    "react-router-config": "1.0.0-beta.4",
    "react-router-dom": "4.2.2",
    "react-router-hash-link": "^1.2.0",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.0",
    "webpack": "3.5.6",
    "webpack-dev-server": "2.8.2",
    "webpack-merge": "4.1.0",
    "webpack-node-externals": "1.6.0"
  },
  "devDependencies": {
    "babel-eslint": "^9.0.0",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^2.10.0",
    "eslint-config-react": "^1.1.7",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.11.1",
    "file-loader": "^1.1.11",
    "image-webpack-loader": "^4.3.1",
    "isomorphic-style-loader": "^4.0.0"
  }
}
